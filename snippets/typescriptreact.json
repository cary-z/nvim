{
  "Console log debug": {
    "prefix": "lg",
    "body": [
      "console.log(",
      "  '%cdebugger%c',",
      "  'background:#ff4d4f;color:#fff;border-radius:999px;padding:2px 8px;font-weight:700;',",
      "  'color:inherit;',",
      "  ${1:message}",
      ");"
    ],
    "description": "Log output to console"
  },
  "Console log": {
    "prefix": "lr",
    "body": ["console.log('\\u001B[31m%s\\u001B[0m', ${1:message});"],
    "description": "Log output to console"
  },
  "Console log JSON": {
    "prefix": "lrj",
    "body": [
      "console.log('\\u001B[31m%s\\u001B[0m', JSON.stringify(${1:message}));"
    ],
    "description": "Log output to console"
  }
}
